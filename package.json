{
  "name": "@paddle/nextjs-starter-kit",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --write --ignore-unknown .",
    "prettier:check": "prettier --check --ignore-unknown .",
    "lint-staged": "lint-staged",
    "test": "pnpm lint && pnpm prettier:check",
    "db:migrate": "npx prisma db push",
    "db:seed": "node prisma/seed.js",
    "db:reset": "npx prisma db push --force-reset && npm run db:seed",
    "db:studio": "npx prisma studio",
    "db:seed-supabase": "node scripts/seed-supabase.js",
    "db:generate": "npx prisma generate",
    "db:deploy": "npx prisma db push --accept-data-loss",
    "test:create-ebooks": "node scripts/create-test-zip.js",
    "test:create-zip": "node scripts/test-batch-upload.js",
    "check-env": "node scripts/check-env.js",
    "test-api": "node scripts/test-api.js",
    "check-storage": "node scripts/check-supabase-storage.js",
    "test-batch-upload": "node scripts/test-batch-upload-complete.js",
    "check-ebooks": "node scripts/check-ebooks-db.js",
    "list-storage": "node scripts/list-storage-files.js",
    "clean-storage": "node scripts/clean-storage.js",
    "test-pdf-conversion": "node scripts/pdf-converter-test.js"
  },
  "git": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
